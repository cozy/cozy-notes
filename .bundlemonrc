{
  "baseDir": "./build",
  "files": [
    {
      "path": "app/notes.<hash>.js",
      "maxSize": "65 KB"
    },
    {
      "path": "intents/notes.<hash>.js",
      "maxSize": "50 KB"
    },
    {
      "path": "public/notes.<hash>.js",
      "maxSize": "40 KB"
    },
    {
      "path": "vendors/notes.<hash>.js",
      "maxSize": "3 MB"
    },
    {
      "path": "app-notes.<hash>.min.css",
      "maxSize": "4 KB"
    },
    {
      "path": "vendors-notes.<hash>.0.min.css",
      "maxSize": "36 KB"
    }
  ],
  "groups": [
    {
      "path": "app/**"
    },
    {
      "path": "public/**"
    },
    {
      "path": "intents/**"
    },
    {
      "path": "vendors/**"
    }
  ],
  "reportOutput": ["github"]
}
