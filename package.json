{
  "name": "cozy-notes",
  "version": "1.0.7",
  "scripts": {
    "tx": "tx pull --all || true",
    "lint": "yarn lint:js && yarn lint:styles",
    "lint:js": "cs lint '{src,test}/**/*.{js,jsx}'",
    "lint:styles": "stylint src/styles --config ./.stylintrc",
    "prebuild": "yarn lint",
    "build": "yarn build:browser",
    "build:browser:dev": "cs build --browser --development",
    "build:browser": "cs build --browser",
    "build:mobile": "cs build --mobile",
    "watch": "yarn watch:browser",
    "watch:browser": "cs watch --browser",
    "watch:mobile": "cs watch --mobile",
    "start": "cs start --hot --browser",
    "deploy": "git-directory-deploy --directory build/ --branch ${DEPLOY_BRANCH:-build} --repo ${DEPLOY_REPOSITORY:-https://cozy-bot:$GITHUB_TOKEN@github.com/cozy/cozy-notes}",
    "test": "cs test --verbose --coverage",
    "cozyPublish": "cs publish --token $REGISTRY_TOKEN --build-commit $(git rev-parse ${DEPLOY_BRANCH:-build})"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cozy/cozy-notes.git"
  },
  "author": "",
  "contributors": [],
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/cozy/cozy-notes/issues"
  },
  "homepage": "https://github.com/cozy/cozy-notes#readme",
  "devDependencies": {
    "babel-preset-cozy-app": "1.2.5",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.9.1",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.12.4",
    "git-directory-deploy": "1.5.1",
    "install-peers": "^1.0.3",
    "prettier": "1.15.3",
    "react-hot-loader": "4.6.5",
    "react-test-renderer": "16.8.1",
    "redux-mock-store": "1.5.3",
    "stylint": "1.5.9"
  },
  "dependencies": {
    "@atlaskit/editor-core": "^113.2.1",
    "@atlaskit/media-core": "^30.0.17",
    "@atlaskit/media-editor": "^37.0.0",
    "bufferutil": "^4.0.1",
    "cozy-bar": "^7.7.8",
    "cozy-client": "^8.10.0",
    "cozy-device-helper": "^1.8.0",
    "cozy-doctypes": "^1.70.0",
    "cozy-realtime": "^3.2.3",
    "cozy-scripts": "^2.0.2",
    "cozy-ui": "^28.5.0",
    "eslint-config-cozy-app": "^1.3.3",
    "lodash.debounce": "^4.0.8",
    "react": "~16.8.1",
    "react-dom": "~16.8.1",
    "react-intl": "2.9.0",
    "react-router-dom": "^5.1.2",
    "snarkdown": "^1.2.2",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0",
    "utf-8-validate": "^5.0.2",
    "uws": "10.148.1"
  }
}
