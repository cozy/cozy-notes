{
  "name": "cozy-notes",
  "version": "1.17.0",
  "scripts": {
    "tx": "tx pull --all || true",
    "lint": "yarn lint:js && yarn lint:styles",
    "lint:js": "cs lint '{src,test}/**/*.{js,jsx}'",
    "lint:styles": "stylint src/styles --config ./.stylintrc",
    "prebuild": "yarn lint",
    "build": "yarn build:browser",
    "build:browser:dev": "cs build --browser --development",
    "build:browser": "cs build --browser",
    "build:mobile": "cs build --mobile",
    "watch": "yarn watch:browser",
    "watch:browser": "cs watch --browser",
    "watch:mobile": "cs watch --mobile",
    "start": "cs start --browser",
    "deploy": "git-directory-deploy --directory build/ --branch ${DEPLOY_BRANCH:-build} --repo ${DEPLOY_REPOSITORY:-https://cozy-bot:$GITHUB_TOKEN@github.com/cozy/cozy-notes}",
    "test": "env NODE_ENV='test' cs test --verbose",
    "cozyPublish": "cs publish --token $REGISTRY_TOKEN --build-commit $(git rev-parse ${DEPLOY_BRANCH:-build})",
    "stack:docker:dev": "COZY_STACK_HOST=cozy.tools docker run --rm -it -p 8080:8080 -p 5984:5984 -e COZY_DISABLE_CSP=1 -v $(pwd)/build:/data/cozy-app/app cozy/cozy-app-dev",
    "kill": "npx kill-port 8888",
    "build:editor": "rm -rf ./src/vendor/editor/ && tsc -w -p src/editor-core/tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cozy/cozy-notes.git"
  },
  "author": "",
  "contributors": [],
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/cozy/cozy-notes/issues"
  },
  "homepage": "https://github.com/cozy/cozy-notes#readme",
  "devDependencies": {
    "@testing-library/react": "9.5.0",
    "@testing-library/react-hooks": "3.2.1",
    "babel-jest": "24.9.0",
    "babel-preset-cozy-app": "1.7.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.5",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.18.3",
    "git-directory-deploy": "1.5.1",
    "install-peers": "1.0.3",
    "prettier": "1.19.1",
    "react-test-renderer": "16.12.0",
    "redux-mock-store": "1.5.4",
    "stylint": "1.5.9",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@atlaskit/editor-core": "145.0.3",
    "@atlaskit/media-core": "32.1.2",
    "@atlaskit/media-editor": "38.1.0",
    "@atlaskit/media-picker": "57.5.0",
    "@atlaskit/smart-card": "14.6.1",
    "@material-ui/core": "4",
    "@material-ui/styles": "4.10.0",
    "cozy-bar": "7.7.8",
    "cozy-client": "23.1.2",
    "cozy-device-helper": "1.8.0",
    "cozy-doctypes": "1.70.0",
    "cozy-flags": "2.1.0",
    "cozy-realtime": "3.13.0",
    "cozy-scripts": "5.1.1",
    "cozy-sharing": "3.3.1",
    "cozy-ui": "45.1.0",
    "eslint-config-cozy-app": "1.3.3",
    "lodash": "4.17.21",
    "prop-types": "^15.7.2",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-intl": "2.9.0",
    "react-router-dom": "5.1.2",
    "rxjs": "5.5.12",
    "styled-components": "3.4.10",
    "url-search-params-polyfill": "7.0.1",
    "utf-8-validate": "5.0.2"
  },
  "resolutions": {
    "prosemirror-model": "1.11.0",
    "prosemirror-tables": "0.9.5"
  }
}
