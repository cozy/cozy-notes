{
  "name": "cozy-notes",
  "version": "1.19.0",
  "scripts": {
    "build:browser:dev": "cs build --browser --development",
    "build:browser": "cs build --browser",
    "build:mobile": "cs build --mobile",
    "build": "yarn build:browser",
    "cozyPublish": "cs publish --token $REGISTRY_TOKEN --build-commit $(git rev-parse ${DEPLOY_BRANCH:-build})",
    "deploy": "git-directory-deploy --directory build/ --branch ${DEPLOY_BRANCH:-build} --repo ${DEPLOY_REPOSITORY:-https://cozy-bot:$GITHUB_TOKEN@github.com/cozy/cozy-notes}",
    "lint:js": "cs lint '{src,test}/**/*.{ts,tsx,js,jsx}'",
    "lint:styles": "stylint src/styles --config ./.stylintrc",
    "lint": "yarn lint:js && yarn lint:styles",
    "prebuild": "yarn lint",
    "stack:docker:dev": "COZY_STACK_HOST=cozy.localhost docker run --rm -it -p 8080:8080 -p 5984:5984 -e COZY_DISABLE_CSP=1 -v $(pwd)/build:/data/cozy-app/app cozy/cozy-app-dev",
    "stack:docker:prod": "COZY_STACK_HOST=cozy.localhost docker run --rm -it -p 8080:8080 -p 5984:5984 -v \"$(pwd)/build\":/data/cozy-app/app cozy/cozy-app-dev",
    "start": "cs start --browser",
    "test": "env NODE_ENV='test' cs test --verbose",
    "tx": "tx pull --all || true",
    "watch:browser": "cs watch --browser",
    "watch:mobile": "cs watch --mobile",
    "watch": "yarn watch:browser"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cozy/cozy-notes.git"
  },
  "author": "",
  "contributors": [],
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/cozy/cozy-notes/issues"
  },
  "homepage": "https://github.com/cozy/cozy-notes#readme",
  "devDependencies": {
    "@babel/eslint-parser": "7.15.0",
    "@babel/preset-env": "7.15.0",
    "@babel/preset-typescript": "7.15.0",
    "@testing-library/react": "9.5.0",
    "@testing-library/react-hooks": "3.7.0",
    "@typescript-eslint/eslint-plugin": "4.29.0",
    "@typescript-eslint/parser": "4.29.0",
    "babel-jest": "24.9.0",
    "babel-preset-cozy-app": "1.11.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.6",
    "eslint": "7.32.0",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.24.0",
    "git-directory-deploy": "1.5.1",
    "install-peers": "1.0.3",
    "prettier": "1.19.1",
    "react-test-renderer": "16.12.0",
    "redux-mock-store": "1.5.4",
    "stylint": "1.5.9"
  },
  "dependencies": {
    "@atlaskit/icon": "21.1.0",
    "@atlaskit/media-core": "32.2.0",
    "@material-ui/core": "4.12.3",
    "@material-ui/styles": "4.11.4",
    "classnames": "2.3.1",
    "cozy-bar": "7.16.0",
    "cozy-client": "27.6.5",
    "cozy-device-helper": "1.12.0",
    "cozy-doctypes": "1.82.2",
    "cozy-editor-core": "134.0.3",
    "cozy-flags": "2.7.2",
    "cozy-realtime": "3.13.0",
    "cozy-scripts": "eslint7",
    "cozy-sharing": "3.12.2",
    "cozy-ui": "51.12.0",
    "eslint-config-cozy-app": "1.6.0",
    "lodash": "4.17.21",
    "prop-types": "15.7.2",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-intl": "2.9.0",
    "react-router-dom": "5.2.0",
    "rxjs": "5.5.12",
    "styled-components": "3.4.10",
    "url-search-params-polyfill": "7.0.1",
    "utf-8-validate": "5.0.5"
  },
  "resolutions": {
    "prosemirror-model": "1.11.0",
    "cozy-editor-core/@atlaskit/editor-tables": "1.1.3"
  }
}
