{
  "name": "cozy-notes",
  "version": "1.13.0",
  "scripts": {
    "tx": "tx pull --all || true",
    "lint": "yarn lint:js && yarn lint:styles",
    "lint:js": "cs lint '{src,test}/**/*.{js,jsx}'",
    "lint:styles": "stylint src/styles --config ./.stylintrc",
    "prebuild": "yarn lint",
    "build": "yarn build:browser",
    "build:browser:dev": "cs build --browser --development",
    "build:browser": "cs build --browser",
    "build:mobile": "cs build --mobile",
    "watch": "yarn watch:browser",
    "watch:browser": "cs watch --browser",
    "watch:mobile": "cs watch --mobile",
    "start": "cs start --hot --browser",
    "deploy": "git-directory-deploy --directory build/ --branch ${DEPLOY_BRANCH:-build} --repo ${DEPLOY_REPOSITORY:-https://cozy-bot:$GITHUB_TOKEN@github.com/cozy/cozy-notes}",
    "test": "env NODE_ENV='test' cs test --verbose",
    "cozyPublish": "cs publish --token $REGISTRY_TOKEN --build-commit $(git rev-parse ${DEPLOY_BRANCH:-build})"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cozy/cozy-notes.git"
  },
  "author": "",
  "contributors": [],
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/cozy/cozy-notes/issues"
  },
  "homepage": "https://github.com/cozy/cozy-notes#readme",
  "devDependencies": {
    "@testing-library/react": "9.5.0",
    "@testing-library/react-hooks": "3.2.1",
    "babel-jest": "24.9.0",
    "babel-preset-cozy-app": "1.7.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.18.3",
    "git-directory-deploy": "1.5.1",
    "install-peers": "1.0.3",
    "prettier": "1.19.1",
    "react-hot-loader": "4.12.19",
    "react-test-renderer": "16.12.0",
    "redux-mock-store": "1.5.4",
    "stylint": "1.5.9"
  },
  "dependencies": {
    "@atlaskit/editor-core": "114.1.4",
    "@atlaskit/media-core": "31.0.1",
    "@atlaskit/media-editor": "37.0.3",
    "@atlaskit/smart-card": "12.6.2",
    "@material-ui/core": "3.9.4",
    "cozy-bar": "7.7.8",
    "cozy-client": "13.3.0",
    "cozy-device-helper": "1.8.0",
    "cozy-doctypes": "1.70.0",
    "cozy-flags": "2.1.0",
    "cozy-realtime": "3.7.0",
    "cozy-scripts": "2.0.2",
    "cozy-sharing": "1.11.1",
    "cozy-ui": "35.22.0",
    "eslint-config-cozy-app": "1.3.3",
    "lodash": "4.17.15",
    "prop-types": "^15.7.2",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-intl": "2.9.0",
    "react-router-dom": "5.1.2",
    "rxjs": "5.5.12",
    "styled-components": "3.4.10",
    "url-search-params-polyfill": "7.0.1",
    "utf-8-validate": "5.0.2"
  }
}
