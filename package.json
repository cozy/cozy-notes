{
  "name": "cozy-notes",
  "version": "1.27.0",
  "scripts": {
    "analyze": "COZY_SCRIPTS_ANALYZER=true yarn build",
    "build:browser:dev": "cs build --browser --development",
    "build:browser": "cs build --browser",
    "build:mobile": "cs build --mobile",
    "build": "yarn build:browser",
    "cozyPublish": "cs publish --token $REGISTRY_TOKEN --build-commit $(git rev-parse ${DEPLOY_BRANCH:-build})",
    "deploy": "git-directory-deploy --directory build/ --branch ${DEPLOY_BRANCH:-build} --repo ${DEPLOY_REPOSITORY:-https://cozy-bot:$GITHUB_TOKEN@github.com/cozy/cozy-notes}",
    "lint:js": "eslint '{src,test}/**/*.{ts,tsx,js,jsx}'",
    "lint:styles": "stylint src/styles --config ./.stylintrc",
    "lint": "yarn lint:js && yarn lint:styles",
    "prebuild": "yarn lint",
    "ci:check": "yarn lint && yarn test && yarn build",
    "stack:docker:dev": "COZY_STACK_HOST=cozy.localhost docker run --rm -it -p 8080:8080 -p 5984:5984 -e COZY_DISABLE_CSP=1 -v $(pwd)/build:/data/cozy-app/app -v $(pwd)/build:/data/cozy-app/notes cozy/cozy-app-dev",
    "stack:docker:prod": "COZY_STACK_HOST=cozy.localhost docker run --rm -it -p 8080:8080 -p 5984:5984 -v \"$(pwd)/build\":/data/cozy-app/app -v \"$(pwd)/build\":/data/cozy-app/notes cozy/cozy-app-dev",
    "start": "cs start --browser",
    "test": "env NODE_ENV='test' cs test --verbose",
    "tx": "tx pull --all || true",
    "watch:browser": "cs watch --browser",
    "watch:mobile": "cs watch --mobile",
    "watch": "yarn watch:browser"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cozy/cozy-notes.git"
  },
  "author": "",
  "contributors": [],
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/cozy/cozy-notes/issues"
  },
  "homepage": "https://github.com/cozy/cozy-notes#readme",
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "12.1.4",
    "@testing-library/react-hooks": "8.0.1",
    "babel-jest": "^27.5.1",
    "babel-preset-cozy-app": "^2.0.2",
    "bundlemon": "1.4.0",
    "eslint-config-cozy-app": "4.2.1",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.24.0",
    "git-directory-deploy": "1.5.1",
    "install-peers": "1.0.3",
    "jest": "28.1.3",
    "jest-environment-jsdom": "28.1.3",
    "prettier": "1.19.1",
    "react-test-renderer": "16.14.0",
    "redux-mock-store": "1.5.4",
    "stylint": "1.5.9"
  },
  "dependencies": {
    "@atlaskit/icon": "21.1.0",
    "@atlaskit/media-core": "32.2.0",
    "@cozy/minilog": "^1.0.0",
    "@material-ui/core": "4.12.3",
    "@material-ui/styles": "4.11.4",
    "classnames": "2.3.1",
    "cozy-bar": "7.19.1",
    "cozy-client": "^34.1.0",
    "cozy-device-helper": "^2.6.0",
    "cozy-doctypes": "1.86.1",
    "cozy-editor-core": "134.0.3",
    "cozy-flags": "2.8.4",
    "cozy-harvest-lib": "7.2.3",
    "cozy-intent": "^2.0.2",
    "cozy-logger": "^1.9.0",
    "cozy-realtime": "4.0.5",
    "cozy-scripts": "8.0.1",
    "cozy-sharing": "^5.0.1",
    "cozy-ui": "^77.6.0",
    "lodash": "4.17.21",
    "prop-types": "15.8.1",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-intl": "2.9.0",
    "react-router-dom": "6.3.0",
    "rooks": "7.1.1",
    "rxjs": "5.5.12",
    "styled-components": "3.4.10",
    "url-search-params-polyfill": "7.0.1",
    "utf-8-validate": "5.0.8"
  },
  "resolutions": {
    "prosemirror-model": "1.11.0",
    "cozy-editor-core/@atlaskit/editor-tables": "1.1.3"
  }
}
