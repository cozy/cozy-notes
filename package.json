{
  "name": "cozy-notes",
  "version": "1.42.0",
  "scripts": {
    "postinstall": "patch-package",
    "analyze": "COZY_SCRIPTS_ANALYZER=true yarn build",
    "build": "cs build --barV7 false",
    "start": "cs start --barV7 false",
    "watch": "cs watch --barV7 false",
    "deploy": "git-directory-deploy --directory build/ --branch ${DEPLOY_BRANCH:-build} --repo ${DEPLOY_REPOSITORY:-https://cozy-bot:$GITHUB_TOKEN@github.com/cozy/cozy-notes}",
    "cozyPublish": "cozy-app-publish --token $REGISTRY_TOKEN --prepublish downcloud --postpublish mattermost",
    "lint:js": "eslint '{src,test}/**/*.{ts,tsx,js,jsx}'",
    "lint:styles": "stylint src/styles --config ./.stylintrc",
    "lint": "yarn lint:js && yarn lint:styles",
    "prebuild": "yarn lint",
    "ci:check": "yarn lint && yarn test && yarn build",
    "stack:docker:dev": "COZY_STACK_HOST=cozy.localhost docker run --rm -it -p 8080:8080 -p 5984:5984 -e COZY_DISABLE_CSP=1 -v $(pwd)/build:/data/cozy-app/app -v $(pwd)/build:/data/cozy-app/notes cozy/cozy-app-dev",
    "stack:docker:prod": "COZY_STACK_HOST=cozy.localhost docker run --rm -it -p 8080:8080 -p 5984:5984 -v \"$(pwd)/build\":/data/cozy-app/app -v \"$(pwd)/build\":/data/cozy-app/notes cozy/cozy-app-dev",
    "test": "env NODE_ENV='test' cs test --verbose",
    "tx": "tx pull --all || true"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cozy/cozy-notes.git"
  },
  "author": "",
  "contributors": [],
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/cozy/cozy-notes/issues"
  },
  "homepage": "https://github.com/cozy/cozy-notes#readme",
  "devDependencies": {
    "@babel/eslint-parser": "7.23.3",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "12.1.4",
    "@testing-library/react-hooks": "8.0.1",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "babel-jest": "^27.5.1",
    "babel-preset-cozy-app": "^2.8.1",
    "bundlemon": "3.1.0",
    "cozy-scripts": "^8.3.0",
    "cozy-tsconfig": "1.2.0",
    "eslint": "8.56.0",
    "eslint-config-cozy-app": "6.1.0",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest": "27.6.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "git-directory-deploy": "1.5.1",
